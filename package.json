    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/svelte": "^7.6.0",
    "@storybook/sveltekit": "^7.6.0",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^2.0.0",
    "@sveltejs/kit": "^2.0.0",
